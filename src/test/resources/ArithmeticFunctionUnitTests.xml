<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Arithmetic Function Suite" parallel="classes" thread-count="1">
    <test name="Arithmetic Function Unit Test">
        <groups>
            <run>
                <include name="SumTest" />
                <include name="SubTest" />
                <include name="MultiplyTest" />
                <include name="DivTest" />
            </run>
        </groups>
        <classes>
            <class name="org.rntgroup.CalcUnitTests.SumLongTest"/>
            <class name="org.rntgroup.CalcUnitTests.SumDoubleTest"/>
            <class name="org.rntgroup.CalcUnitTests.SubDoubleTest"/>
            <class name="org.rntgroup.CalcUnitTests.MultiplyDoubleTest"/>
            <class name="org.rntgroup.CalcUnitTests.MultiplyLongTest"/>
            <class name="org.rntgroup.CalcUnitTests.DivLongTest"/>
        </classes>
    </test>
</suite>